body
  background-color #EEEEEE
  
input[type="text"],
input[type="password"]
  margin 10px 5px
  
a 
  color #222222
  text-decoration: none

a:hover 
  text-decoration:none
  cursor:pointer
  
ul
  list-style none
  
.header
  background-color #222222
  padding 10px 20px 0 20px
  height 90px

.footer
  clear both
  margin-top 100px
  padding 0 20px
  color #999999
  p
    text-align center
  
.content
  //background-color #222222
  padding 0 20px
  
.loginbox
  margin-top 30px
  
.menu
  color #999999
  margin-top 30px
  span
    font-size 1.5em
  ul
    left -120px
    
.quicklinks
  position relative
  //clear both
  //height 20px
  width 100%
  left 10px
  top 55px
  a
    float left
    color #BBBBBB
    margin 0 10px
  a:hover
    color white
  
.slideshow
  padding-top 10px
  margin auto
  overflow auto
  position relative
  width 100%
  button
    position absolute
    bottom 10%
    right 10%
    
.nivo-caption 
  background none;
  color: #333;
  //height: 65px;
  padding: 8px;
  position: absolute;
  right: 38px;
  top: 40px;
  
  z-index: 8;
  p
    width: 60%
    margin 20px
    font-size 300%
    line-height 95%
    letter-spacing 80%
  button
    clear both

    

.nivo-html-caption
  display none    

.bigcover
  //background-color #555555
  //overflow auto
  position relative
  img
    width 90%
  span
    position absolute
    float left
    top 20%
    left 10%
    font-size 10em
  button
    position absolute
  
.lowertabs
  //background-color #666666
  clear both
  margin 10px 0
  position relative
  div
    float left
    padding 1% 0
    width 33.33%
    background-color #7BB739
    h4
      text-align center
    
.two-col
  //overflow auto
  .col-1
    float left
  .col-2
    float right
    
.stats
  width 90%
  position relative
  margin auto
  div
    float left
    
.farmbox
  overflow auto
  clear both
  margin auto
  margin-top 10px
  width 90%
  
.staticon
  float left
  width 100px
  height 100px
  border 1px solid
  span, img
    margin-left 30px
    
#container
  position relative
  width auto
  height 300px
    
.chartbox
  position relative
  height 300px
  border 1px solid
  width 80%
  display none
  margin auto
  
.statbox
  position relative
  width 24%
  min-width 150px
  height 200px
  border 1px solid
  
.statbox:hover
  background-color #DDFFDD

.btn-blue
	color: #FFFFFF
	text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25)
	background-color: #329A9F
	*background-color: #329A9F
	background-image: -moz-linear-gradient(top, #13CBD4, #329A9F)
	background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#13CBD4), to(#329A9F))
	background-image: -webkit-linear-gradient(top, #13CBD4, #329A9F)
	background-image: -o-linear-gradient(top, #13CBD4, #329A9F)
	background-image: linear-gradient(to bottom, #13CBD4, #329A9F)
	background-repeat: repeat-x
	border-color: #329A9F
	border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25)


.btn-blue:hover,
.btn-blue:active,
.btn-blue.active,
.btn-blue.disabled,
.btn-blue[disabled] 
	color: #FFFFFF
	background-color: #329A9F
	*background-color: #003bb3

  
.btn-green 
	color: #FFFFFF
	text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25)
	background-color: #7BB739
	background-image: -moz-linear-gradient(top, #A7FA4E, #7BB739)
	background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#A7FA4E), to(#7BB739))
	background-image: -webkit-linear-gradient(top, #A7FA4E, #7BB739)
	background-image: -o-linear-gradient(top, #A7FA4E, #7BB739)
	background-image: linear-gradient(to bottom, #A7FA4E, #7BB739)
	background-repeat: repeat-x
	border-color: #7BB739
	border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25)
	//filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#A7FA4E', endColorstr='#7BB739', GradientType=0)
	//filter: progid:DXImageTransform.Microsoft.gradient(enabled=false)

.btn-green:hover,
.btn-green:active,
.btn-green.active,
.btn-green.disabled,
.btn-green[disabled]
	color: #FFFFFF
	background-color: #7BB739
	*background-color: #003bb3
